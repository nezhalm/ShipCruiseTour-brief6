#apache redirecte
<IfModule mod_rewrite.c>
  Options -Multiviews
  RewriteEngine On
  RewriteBase /ShipCruiseTour-brief6/public
# si le file ou le dossier  est introuvable redirect
# to index.php in public dans cette url:localhost/shareposts/public/:::::  
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteRule  ^(.+)$ index.php?url=$1 [QSA,L]
  # url=$1 npus permet decrire /posts fblast url=posts
</IfModule>